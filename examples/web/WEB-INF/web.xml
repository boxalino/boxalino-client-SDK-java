        <servlet-class>com.boxalino.examples.DataSplitFieldValues</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DataTransactions</servlet-name>
        <servlet-class>com.boxalino.examples.DataTransactions</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ParametrizedRequest</servlet-name>
        <servlet-class>com.boxalino.examples.ParametrizedRequest</servlet-class>
