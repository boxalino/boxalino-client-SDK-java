<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Search2ndPage</servlet-name>
        <servlet-class>com.boxalino.examples.Search2ndPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DataBasic</servlet-name>
        <servlet-class>com.boxalino.examples.DataBasic</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DataCategories</servlet-name>
        <servlet-class>com.boxalino.examples.DataCategories</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DataCustomers</servlet-name>
        <servlet-class>com.boxalino.examples.DataCustomers</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Search2ndPage</servlet-name>
        <url-pattern>/Search2ndPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DataBasic</servlet-name>
        <url-pattern>/DataBasic</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DataCategories</servlet-name>
        <url-pattern>/DataCategories</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DataCustomers</servlet-name>
        <url-pattern>/DataCustomers</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
